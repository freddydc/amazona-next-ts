root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.{ts,tsx,html,css}]
indent_style = space

[*.ts]
indent_size = 2

[*.tsx]
indent_size = 2

[*.html]
indent_size = 2

[*.css]
indent_size = 2

[package.json]
indent_style = space
indent_size = 2
